{
    "name": "bjerke/laravel-headless-ecommerce",
    "description": "A headless ecommerce package for Laravel",
    "keywords": [
        "jesperbjerke",
        "headless",
        "api",
        "ecommerce",
        "shopping",
        "shop",
        "rest",
        "laravel"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jesper Bjerke",
            "email": "jesper.bjerke@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-intl" : "*",
        "illuminate/support": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/bus": "^8.0",
        "illuminate/console": "^8.0",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-translatable": "^4.6",
        "spatie/laravel-medialibrary": "^9.0",
        "bjerke/api-query-builder": "^1.1",
        "bjerke/laravel-bread": "^1.8",
        "bjerke/laravel-enums": "^1.0",
        "kalnoy/nestedset": "^5.0",
        "laravel/scout": "^8.5.1",
        "moneyphp/money": "^3.3.1",
        "league/omnipay": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bjerke\\Ecommerce\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bjerke\\Ecommerce\\EcommerceServiceProvider"
            ],
            "aliases": {
                "PriceHelper": "Bjerke\\Ecommerce\\Facades\\PriceHelper",
                "PaymentHelper": "Bjerke\\Ecommerce\\Facades\\PaymentHelper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
